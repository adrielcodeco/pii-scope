{
  "compilerOptions": {
    "stripInternal": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "preserveConstEnums": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "strict": true,
    "module": "commonjs",
    "target": "es2016",
    "lib": ["esnext"]
  }
}
